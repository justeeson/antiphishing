{
  "manifest_version": 2,

  "name": "Anti Phishing",
  "description": "This extension saves the user from phishing attempts",
  "version": "1.0",
  
  
  "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },
  
  "content_scripts": [
    {
      "run_at" :"document_end",
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  
  "browser_action": {
  	"default_title": "Anti Phishing Module",
	  "default_popup": "popup.html"
  },
  
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}